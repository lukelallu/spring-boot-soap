	<xsd:complexType name="ChildSupportPayment">
		<xsd:annotation>
			<xsd:documentation>WebSeries type code: DACHCSP Entry class: CCD Addenda Record: DED</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHPaymentBase">
				<xsd:sequence>
					<xsd:element name="agency" type="tns:ChildSupportAgency">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The governmental agency responsible for the case</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="absenteeParent" type="tns:AbsenteeParent">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The non-custodial parent’s details, e.g. name and SSN</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="caseIdentifier" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The case identifier element is the IV-D case number or court order number. The case identifier always refers to the identification number of the case in the state receiving the EFT/EDI transaction </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="payDate" type="xsd:date">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The pay date element provides the obligor’s (non-custodial parent’s) pay date; the date the income was withheld from the employee’s paycheck.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FIPSCode" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The Federal Information Process Standard (FIPS) code refers to the FIPS Code of the SDU receiving the transaction. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="employmentTerminated" type="tns:YesNoBlankIndicator" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The employment termination indicator is used to notify the child support enforcement agency that an individual’s employment has terminated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="medicalSupportIndicator" type="tns:MedicalSupportIndicator" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The medical support indicator indicates whether the employer offers family medical insurance coverage. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="formatttedAddenda" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Addenda information formatted based on other elements. This is system generated and not user enetered.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FreeformAddenda">
		<xsd:annotation>
			<xsd:documentation>Type for freeform addeda info</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="tns:BasicAddendaInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RemittanceAddendaInfo">
		<xsd:annotation>
			<xsd:documentation>Remittance addenda is formatted by system based on supplied fields</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="totalInvoiceAmount" type="btamt:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total amount of the invoice that is addressed by this payment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="termsDiscount" type="btamt:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Amount deducted from total based on discount terms e.g. 2/10 net 30</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="adjustmentAmount" type="btamt:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Amount deducted from total based on adjustment e.g. missing or damaged goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="adjustmentReason" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Reason code for adjustment amount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="referenceNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Reference number for invoice</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="referenceNumberType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"> Type of reference number e.g invoice number, bill of lading, etc.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"> User entered payment description for incluson in addenda</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CorporateVendorPayment">
		<xsd:annotation>
			<xsd:documentation>WebSeries type code: DACHCVP Entry class: CCD Addenda Record: NTE/RMT</xsd:documentation>
