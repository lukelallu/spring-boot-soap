				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lastName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Absentee parent responsible for child support</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SSN" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Social security number of absentee parent responsible for child support</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
