						</xsd:annotation>
					</xsd:element>
					<xsd:element name="syndryinfo" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Sundry Information</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="tranCode" type="xsd:long" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">TRAN_CODE for Payment Details</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EFTPaymentFieldsCCD">
