<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.bottomline.com/webseries/schema/ps/platform/payment/ach/v1" xmlns:btpay="http://www.bottomline.com/webseries/schema/ps/platform/payment/v1" xmlns:btws="http://www.bottomline.com/webseries/schema/ps/platform/common/v1" xmlns:btacct="http://www.bottomline.com/webseries/schema/ps/platform/account/v1" xmlns:btamt="http://www.bottomline.com/webseries/schema/ps/platform/amount/v1" xmlns:btbank="http://www.bottomline.com/webseries/schema/ps/platform/bank/v1" targetNamespace="http://www.bottomline.com/webseries/schema/ps/platform/payment/ach/v1" elementFormDefault="qualified" version="1.0">
	<xsd:import namespace="http://www.bottomline.com/webseries/schema/ps/platform/common/v1" schemaLocation="platformCommon.xsd"/>
	<xsd:import namespace="http://www.bottomline.com/webseries/schema/ps/platform/account/v1" schemaLocation="platformAccount.xsd"/>
	<xsd:import namespace="http://www.bottomline.com/webseries/schema/ps/platform/amount/v1" schemaLocation="platformAmount.xsd"/>
	<xsd:import namespace="http://www.bottomline.com/webseries/schema/ps/platform/bank/v1" schemaLocation="platformBank.xsd"/>
	<xsd:import namespace="http://www.bottomline.com/webseries/schema/ps/platform/payment/v1" schemaLocation="platformPayment.xsd"/>
	<xsd:simpleType name="AddendaText">
		<xsd:annotation>
			<xsd:documentation>Single line of addenda text. Limited to 80 chars</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EntryClass">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">SEC  entry class codes for ACH payments</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ARC"/>
			<xsd:enumeration value="CCD"/>
			<xsd:enumeration value="CIE"/>
			<xsd:enumeration value="CTX"/>
			<xsd:enumeration value="IAT"/>
			<xsd:enumeration value="POP"/>
			<xsd:enumeration value="POS"/>
			<xsd:enumeration value="PPD"/>
			<xsd:enumeration value="RCK"/>
			<xsd:enumeration value="TEL"/>
			<xsd:enumeration value="WEB"/>
			<xsd:enumeration value="XCK"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="YesNoBlankIndicator">
		<xsd:annotation>
			<xsd:documentation>yes/No or explicit "blank" value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="N"/>
			<xsd:enumeration value="blank"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MedicalSupportIndicator">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Valid values for medical support indicator for chils support payment</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="N"/>
			<xsd:enumeration value="W"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="BatchConfirmation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Confirmation details (name and number)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="number" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Confirmation Id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="timestamp" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Confirmation timestamp</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BatchFileInfo">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Details of batch file (incoming or outgoing)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Confirmation Id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="timestamp" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Confirmation timestamp</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BatchTotals">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Structure which contains various totals associated with a payment batch. These in clude both amounts and item counts.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="debits" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of debits in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="debitAmount" type="btamt:Amount">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total amount of debits in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="credits" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of credits in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="creditAmount" type="btamt:Amount">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total amount of credits in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="expired" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total expired</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="holds" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total of number holds</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="items" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of items in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="needsRepair" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of items needing repair</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="prenotes" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of prenotes</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="prenoteAmount" type="btamt:Amount">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total amount of all prenotes in the batch</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="zeroDollar" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="zeroDollarLive" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total number of zero dollar transactions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="variance" type="btamt:Amount">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Total variance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ACHCompanyInfo">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Basic information identifying ACH company</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Company Id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Company name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fullName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Company full name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Comapny description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ACHBankDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Bank information for a beneficiry bank</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="bankName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"> Bank name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bankIdentifier" type="btbank:BankIdentifier">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Identifies invividual bank by routing/sort code. Contains the sort code and type/format of that code. Supported types include: ABA, BIC, BSB</xsd:documentation>
					<xsd:documentation xml:lang="en">Mapping note for ACH IAT Addenda bank qualifier: 01 = ABA, 02=BIC, 03=IBAN. If not specified ABA is default.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="country" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Two letter ISO country code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bankCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">WebSeries bank code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IATCorrespondentBank">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Details for a correspondent bank specified for International ACH Transaction (IAT)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHBankDetails">
				<xsd:sequence>
					<xsd:element name="address1" type="xsd:string"/>
					<xsd:element name="address2" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ACHBatchOriginator">
		<xsd:annotation>
			<xsd:documentation>Originator information for a batch of ACH payments</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="company" type="tns:ACHCompanyInfo">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Company id and name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="btws:Address1"/>
			<xsd:element name="bank" type="tns:ACHBankDetails"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ACHBatchDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Details of a batch of ACH payments</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="btpay:PaymentDetailBase">
				<xsd:sequence>
					<xsd:element name="originator" type="tns:ACHBatchOriginator">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Company associated with batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="entryClass" type="tns:EntryClass" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Entry class for payments in batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="entryDescription" type="xsd:string" minOccurs="0" >
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Entry description for batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="offestAccountData" type="btacct:AccountData">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Offset account for batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="offestBankId" type="btbank:BankIdentifier" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Bank Id associated with offset account for batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="totals" type="tns:BatchTotals">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">User entered comment for the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="descriptiveDate" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Descriptive for batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="comment" type="btpay:Comment" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">User entered comment for the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="specialInstructions" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Instructions for the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="immediateOrigin" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Routing Number of the sending point that is sending the file</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="immediateOriginName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The name of the sending point that is sending the file</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="immediateDestination" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Routing Number of the receiving point to which the file is being sent</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="immediateDestinationName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">The name of the receiving point for which the file is being sent</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="serviceClassCode" type="xsd:long" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Service class code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="firstConfirmation" type="tns:BatchConfirmation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">First confirmation for the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="secondConfirmation" type="tns:BatchConfirmation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Second confirmation for the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="inboundFile" type="tns:BatchFileInfo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Inbound file data</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="outboundFile" type="tns:BatchFileInfo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Outbound file data</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="settlementaccountnumber" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Settlement Account Number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="settlementaccountname" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Settlement Account Name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ACHPaymentBase" abstract="true">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Details of an individual ACH payment (contained within a batch)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="btpay:PaymentDetailBase">
				<xsd:sequence>
					<xsd:element name="sequenceNumber" type="xsd:long">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Sequence number within the batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="batchPaymentId" type="btpay:PaymentId">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Data used to uniquely identify the batch payment to which this payment belongs</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="originator" type="tns:ACHBatchOriginator">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Company associated with batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="entryClass" type="tns:EntryClass">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Entry class for payments in batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="entryDescription" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Entry description for batch</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="debitCreditIndicator" type="btpay:DebitCreditIndicator">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Debit or Credit Indicator (DR, CR)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="amount" type="btamt:CurrencyAmount">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Total amount for this transaction. Depending on type there may be additional amounts in detail (e.g. tax payments or adjustments to vendor payments)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="onHold" type="xsd:boolean" default="false">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">True indicates payment is on hold</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="prenote" type="xsd:boolean" default="false">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">True indicates that the prenote payment will be made.  False indicates that there is no prenote associated to the live payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="prenoteDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en"/>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="zeroDollarLive" type="xsd:boolean" default="false">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">True  indicates that the payment is a zero dollar live tax payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="startDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Start date of recurring payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Expiration date of recurring payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transactionComment" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">User comment. Not transmitted with payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="BasicAddendaInfo">
		<xsd:annotation>
			<xsd:documentation>Simple addemda code and text</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="addendaCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Addenda code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="addendaText" type="tns:AddendaText">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Addenda text. Max 80 charchters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="ACHBeneAccount">
		<xsd:annotation>
			<xsd:documentation>Account and type for bene (external) accounts</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="number" type="xsd:string"/>
			<xsd:element name="type" type="btacct:AccountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Account type, e.g.  Checking (DD), Savings (SV), General Ledger (GL)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:group name="AccountAndBankInfo">
		<xsd:annotation>
			<xsd:documentation>Group for account and associated bank data</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account" type="tns:ACHBeneAccount">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Account details for beneficiary account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank" type="tns:ACHBankDetails">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Bank details for beneficiary bank</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="NameAndId">
		<xsd:annotation>
			<xsd:documentation>Common group for name and id fields</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="id" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="ACHBeneficiary">
		<xsd:annotation>
			<xsd:documentation>Basic ACH Beneficiary information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="tns:NameAndId">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Company, consumer or employee name and Id</xsd:documentation>
				</xsd:annotation>
			</xsd:group>
			<xsd:group ref="tns:AccountAndBankInfo">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Beneficiary bank and account information</xsd:documentation>
				</xsd:annotation>
			</xsd:group>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ACHBeneficiaryDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Details of ACH batch child beneficiary, including amount debited or credited</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:IATRecipient">
				<xsd:sequence>
					<xsd:element name="paymentId" type="btpay:PaymentId">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Payment Id of the batch child transaction related to this beneficiary.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="paymentSequenceNumber" type="xsd:long" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">This may be popultade with sequence number within the batch of the child payment associated with this beneficiary.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="debitCreditIndicator" type="btpay:DebitCreditIndicator">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Debit or Credit Indicator (DR, CR)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="amount" type="btamt:CurrencyAmount">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Total amount for this transaction. Depending on type there may be additional amounts in detail (e.g. tax payments or adjustments to vendor payments)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="beneAddressBookInfo" type="btpay:BeneAddressBookInfo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Optional elements including details of beneficiary address book entry used to create this payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="onUsIndicator" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">On us indicator (Y/N)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="tranCode" type="xsd:long" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">This may be populated with TRAN_CODE associated with this beneficiary.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="customFields" type="btws:nameValuePair" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
