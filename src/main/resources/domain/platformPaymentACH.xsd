			<xsd:documentation>WebSeries type code: DACHTP Entry class: CCD Addenda Record: TXP</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHPaymentBase">
				<xsd:sequence>
					<xsd:element name="taxAuthority" type="tns:TaxAuthority">
						<xsd:annotation>
							<xsd:documentation>Government entity receiving tax</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxType" type="tns:TaxType">
						<xsd:annotation>
							<xsd:documentation>Details of tax being paid</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxpayer" type="tns:Taxpayer">
						<xsd:annotation>
							<xsd:documentation>Person or entity paying the tax</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxAmount1" type="tns:TaxAmount">
						<xsd:annotation>
							<xsd:documentation>Details of portion of overall tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxAmount2" type="tns:TaxAmount" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of portion of overall tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxAmount3" type="tns:TaxAmount" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of portion of overall tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxAmount4" type="tns:TaxAmount" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of portion of overall tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxAmount5" type="tns:TaxAmount" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of portion of overall tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxPeriodEndDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>End date for period covered by tax payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="taxVerification" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Taxpayer verfication or reference number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
