			<xsd:documentation xml:lang="en">DGB EFT Payment Fields</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHPaymentBase">
				<xsd:sequence>
					<xsd:element name="returnaccountnumber" type="xsd:string" minOccurs="0">
