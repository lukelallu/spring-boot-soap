	<xsd:complexType name="ACHPaymentDetail">
		<xsd:annotation>
			<xsd:documentation>Generic ACH payment detail. Includes beneficiary information but no addenda. Beneificiary information includes extended information for IAT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHPaymentBase">
				<xsd:sequence>
					<xsd:element name="beneficiaryInfo" type="tns:IATRecipient">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Benficiary info</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ACHBatchWithBeneDetails">
		<xsd:annotation>
			<xsd:documentation>Generic ACH batch with children</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHBatchDetails">
				<xsd:sequence>
					<xsd:element name="beneficiaryDetail" type="tns:ACHBeneficiaryDetails" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Benficiary info</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="customFields" type="btws:nameValuePair" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
		<xsd:complexType name="NachaFileImport">
		<xsd:annotation>
			<xsd:documentation>WebSeries type code:</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tns:ACHPaymentBase">
				<xsd:sequence>
					<xsd:element name="beneficiaryCompany" type="tns:ACHBeneficiary"/>
					<xsd:group ref="tns:BasicAddendaInfo" minOccurs="0"/>
					<xsd:element name="cardTransactionTypeCode" type="xsd:string" minOccurs="0"/>
					<xsd:group ref="tns:MultiAddendaInfo" minOccurs="0"/>
					<xsd:element name="paymentTypeCode" type="tns:PaymentTypeCode" minOccurs="0"/>
					<xsd:element name="processControl" type="xsd:string" minOccurs="0"/>
					<xsd:element name="terminalCity" type="xsd:string" minOccurs="0"/>
					<xsd:element name="terminalState" type="xsd:string" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="MultiAddendaInfo">
		<xsd:annotation>
			<xsd:documentation>Used for payment types which support multiple addenda. For USACH, CTX supports multi-addenda</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="numAddendaRecords" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Defines the Total Number of addenda records for the transaction. This will be the total number, regardless of how many addenda are returned within the response. (Actual Addenda returned may be limiited)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="multiAddendaEntry" type="tns:MultiAddendaEntry" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>List of addenda entries, including the sequence number of the addenda, and the addenda text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="MultiAddendaEntry">
		<xsd:sequence>
			<xsd:element name="multiAddendaRecNum" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Defines the sequence number of this addenda</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="multiAddendaText" type="tns:AddendaText">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Addenda text. Max 80 charchters</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="PaymentTypeCode">
		<xsd:annotation>
			<xsd:documentation>Payment Type Code, used to identify Single or Recurring for Web and Tel payments</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
</xsd:schema>
