	<xsd:complexType name="CreateUserGroupRequest">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Create a new user group</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="requestHeader" type="btmsg:RequestHeader" nillable="false">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">WebSeries standard request fields</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="userGroup" type="btug:UserGroup" nillable="false">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Details of the user group to be created.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="deafultPassword" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Deprecated. Please use defaultPassword.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="defaultPassword" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Default password to be used for initial admin users that are created automatically. This password is set as expired and admins are forced to change it upon first login.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="clientMaintenance" type="xsd:boolean" default="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">This is set to true is requesting user (from request header) is bank administrator (CBAO or CSU) and user group is a customer. If set to false the requesting user group should be CBAO and the request is to set up a Customer Service Unit (CSU) within the bank</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CreateUserGroupResponse">
		<xsd:sequence>
			<xsd:element name="responseHeader" type="btmsg:ResponseHeader">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">WebSeries standard response fields</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CloneUserGroupRequest">
		<xsd:annotation>
